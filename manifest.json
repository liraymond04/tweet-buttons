{
  "manifest_version": 2,
  "name": "Tweet Buttons",
  "version": "1.0",
  "description": "Add buttons for post interactions on a tweet",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
